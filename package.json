{
	"name": "skiplist",
	"version": "0.0.1",
	"description": "skiplist implementation for javascript",
	"main": "index.js",
	"scripts": 
	{
		"test": "mocha -R spec test/*.js && mocha --require blanket -R travis-cov test/*.js",
		"test-cov": "mocha --require blanket -R travis-cov test/*.js",
		"coverage": "mocha --require blanket -R html-cov test/*.js > test/coverage.html",
		"blanket":
		{
			"pattern": "//^((?!\/node_modules|test\/).)*$/ig"
		},
		"travis-cov": { "threshold": 95 }
	},
	"repository": { "type": "git", "url": "git@github.com:ceejbot/skiplist.git" },
	"keywords": 
	[
		"skiplist",
		"data structures"
	],
	"devDependencies":
	{
		"mocha": "*",
		"chai": "*",
		"blanket": "*",
		"travis-cov": "*"
	},
	"author": "C J Silverio <ceejceej@gmail.com> (http://ceejbot.tumblr.com/)",
	"license": "MIT"
}
